package Multithreading;

/**
 * Created by Abhishek Jaiswal on 27-01-2016.
 * implementing threading by extending Thread
 */
public class ImplementationUsingThread extends Thread {
    public void run() {
        System.out.print("A new thread created...");
    }
    public static void main(String args[]) {
        new ImplementationUsingThread().start();
    }
}
